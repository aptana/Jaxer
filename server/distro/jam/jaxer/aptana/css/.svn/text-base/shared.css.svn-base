#navigation ul { right: 0; } 

* html #navigation ul { right: 10px; } 

#navigation div {
	margin: 0 auto;
}

#navigation div { position: relative; right:25px; }

#navigation {
	height: 74px;
	position: relative;
	text-align: left;
	background-image: url('../images/header_background_2.gif');
	background-repeat: repeat-x;
	padding: 0 10px;
    min-width:700px;	
}

#navigation ul {
	padding: 0;
	margin: 0;
	list-style: none;
	position: absolute;
	top: 0;
}

#navigation li {
	float: left;
	padding: 0;
	margin: 0;
}

#navigation ul a:link, #navigation ul a:visited {
	display: block;
	float: left;
	padding: 29px 15px;
	text-decoration: none;
	color: #FFFFFF;

	height: 71px;
	voice-family: "\"}\"";
	voice-family: inherit;
	height: 13px;
} #ignored {}

#navigation ul li a:hover {
	background: #454D5C;
	color: #fff;
}

#navigatation ul li a.selected {
	background: #454D5C;
	color:inherit;
}

#header {
	CLEAR: both;
	MARGIN: 0px;
	POSITION: relative;
}

#header H1 {
	LEFT: 0px;
	MARGIN: 1px 0px 0px 0px;
	POSITION: absolute;
	TOP: 0px;
	padding-top: 16px;
	padding-right: 10px;
	padding-bottom: 10px;
	padding-left: 40px;
	BACKGROUND: url(../images/title_2.gif) no-repeat 54px 12px;
}
#header H1 A {
	color: #323943; DISPLAY: block; FONT-SIZE: 1px; WIDTH: 171px; TEXT-INDENT: -7000em; HEIGHT: 40px; TEXT-DECORATION: none
}
#header H1 A:hover {
	color: #323943; DISPLAY: block; FONT-SIZE: 1px; WIDTH: 171px; TEXT-INDENT: -7000em; HEIGHT: 40px; TEXT-DECORATION: none
}
#header FORM {
	RIGHT: 30px; FONT-FAMILY: tahoma, arial, sans-serif; POSITION: absolute; TOP: 18px;
	background-image: url('../images/search_background.gif');
	background-repeat: no-repeat;
	width: 220px;
	height: 30px;
	text-align:left;
}

#header FORM LABEL {
	FONT-WEIGHT: bold
}
#header FORM .searchButton, #submit {
	FONT-SIZE: 10px;
	FONT-FAMILY: tahoma, arial, sans-serif;
	background-color: #F99432; color: #FFFFFF;
	border:1px solid #CCCCCC;
}
#header FORM FIELDSET {
	PADDING:0px;
	MARGIN:0px;
}

#header FORM #searchInput, #header FORM FIELDSET #s {
	padding:1px;
	margin-left: 10px;
	margin-right: 10px;
	margin-top:3px;
	FONT-WEIGHT: normal;
	FONT-SIZE: 100%;
	WIDTH: 130px;
	FONT-FAMILY: tahoma, arial, sans-serif;
	background-color:transparent;
	border:1px solid white;
}

