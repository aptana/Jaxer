/* TOC (primarily used in wiki, but could be used elsewhere) */
#toc {
	PADDING: 10px; FONT-SIZE: 0.9em; MARGIN: 0px 0px 0px 0px; BACKGROUND-COLOR: #E9E9E9; color : inherit;
}
#toc TD {
	padding: 10px;
}
#toc H2 {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; FONT-SIZE: 1.4em; PADDING-BOTTOM: 2px; MARGIN: 0px; PADDING-TOP: 0px; border-bottom: none;
}
#toc .tocindent {
	MARGIN-LEFT: 1em
}
#toc .tocline {
	MARGIN-BOTTOM: 0px
}
#toc P {
	MARGIN: 0px
}
#toc .toctoggle {
	FONT-SIZE: 90%
}
#toc .editsection {
	MARGIN-TOP: 0.7em; DISPLAY: none; FONT-SIZE: 94%
}
#toc UL {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-TOP: 0px
}
#toc LI {
	MARGIN-LEFT: 1.2em; LIST-STYLE-TYPE: none; padding-bottom:2px; padding-top:2px;
}

ol { list-style-type: decimal; }
ol ol { list-style-type: lower-alpha; }
ol ol ol { list-style-type: lower-roman; }
ol ol ol ol { list-style-type: decimal; }
ol ol ol ol ol { list-style-type: lower-alpha; }
ol ol ol ol ol ol { list-style-type: lower-roman; }
ul { list-style-type: square; }
ul ul { list-style-type: disc; }
ul ul ul { list-style-type: circle; }
ul ul ul ul { list-style-type: square; }
ul ul ul ul ul { list-style-type: disc; }
ul ul ul ul ul ul { list-style-type: circle; }

.menuItems {
	list-style-type:none;
}
